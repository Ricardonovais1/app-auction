<div class="mx-auto p-4" style="width: 75%; background: #e9ecef">
<h1>Lances vencedores</h1>



<% if @expired_lots.any?  %>

  <% @expired_lots.each do |lot| %>

    <% if lot.bids.any? %>

      <p><strong>Arrematante: </strong> <%= lot.successful_bid_data %></p>

    <% end %>

  <% end %>

  <% else %>

    <h3><%= 'Nenhum lance vencedor' %></h3>

<% end %>

</div>